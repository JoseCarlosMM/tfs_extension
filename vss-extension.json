{
     "manifestVersion": 1,
     "id": "dashboard-extension",
     "version": "0.1.0",
     "name": "Dashboard extension",
     "description": "A Visual Studio Team Services dashboard for Scrum projects.",
     "publisher": "url2015",
     "targets": [
         {
             "id": "Microsoft.VisualStudio.Services"
         }
     ],
      "icons": {
         "default": "images/logo.png"
     },
     "scopes": ["vso.build", "vso.work", "vso.code"],
        "contributions": [
         {
            "id": "dashboard-group",
            "type": "ms.vss-web.hub-group",
            "description": "Dashboard group",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Dashboard",
                "order": 100
            }
        },
        {
            "id": "dashboard-hub",
            "type": "ms.vss-web.hub",
            "description": "Dashboard hub.",
            "targets": [
                ".dashboard-group"
            ],
            "properties": {
                "name": "Dashboard",
                "order": 100,
                "uri": "page.html"
            }
        }
     ],
      "files": [
         {
             "path": ".", "addressable": true
         }
     ]
}